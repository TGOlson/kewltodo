<%= link_to 'create a new todo', new_todo_path %>

<% @todos.each do |todo| %>
	<p><%= todo.description %>
	<%= todo.complete %>
	<%= link_to 'complete', todo_path(id: todo.id, todo: { complete: !todo.complete }), method: :put %>
	<%= link_to 'delete', todo_path(todo), method: :delete %></p>
	<% end %>